{
  "expo": {
    "name": "Lunge",
    "slug": "LungeFitnessTracker",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.jpeg",
    "userInterfaceStyle": "light",
    "scheme": "lunge-fitness-tracker",
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses FaceID to secure your data.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to upload profile pictures.",
        "UIBackgroundModes": [
          "fetch",
          "fetch"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "UIDeviceFamily": [
          1
        ]
      },
      "bundleIdentifier": "com.nafle.LungeFitnessTracker",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3289864451968314~2406960469"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-build-properties",
      "react-native-google-mobile-ads"
    ],
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "7f46a2f5-8963-44d4-bba2-2ee173bca3e1"
      }
    }
  }
}
